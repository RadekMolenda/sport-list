<%= erb(:sports, layout: false, locals: { active: sport.id, sports: sports }) %>
<div class="large-4 columns left">
  <ul class="side-nav">
    <% events.each do |event| %>
      <li class="<%= event.id == active ? 'active' : '' %>"><a href="/sports/<%= sport.id %>/events/<%= event.id %>"><%= event.title %></a></li>
    <% end %>
  </ul>
</div>
